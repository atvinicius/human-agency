# OpenRouter API (server-side only - used in Vercel API routes)
OPENROUTER_API_KEY=sk-or-v1-your-key-here

# Supabase - Client side (VITE_ prefix exposes to frontend for real-time subscriptions)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=sb_publishable_your-publishable-key

# Supabase - Server side (NO VITE_ prefix - only available in API routes)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_KEY=sb_secret_your-secret-key
